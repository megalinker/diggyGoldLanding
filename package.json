{
  "name": "vite-react-motoko",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" \"backend/**/*.mo\"",
    "start": "run-p start:frontend",
    "start:frontend": "vite --config vite.config.ts --port 3000"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^10.0.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.0.1",
    "dotenv": "^16.3.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.83.0",
    "typescript": "^5.1.3",
    "vite": "^6.0.3",
    "vite-plugin-environment": "^1.1.3"
  }
}
